{
  "name": "io.offstage.frontend-starter-kit",
  "version": "1.0.0",
  "description": "WebPack React Sass Offstage's Frontend Starter Kit",
  "main": "index.js",
  "scripts": {
    "setup": "npm install",
    "test": "test",
    "build": "npm run fixes && NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config=./webpack.config.js -p",
    "dev": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors --content-base=build/ --port=8081 --config=./webpack.config.js",
    "css-lint": "echo \"=========== CSS-LINT ===========\"; stylefmt -r \"src/sass/**/*.scss\" && stylelint \"src/sass/**/*.scss\" --syntax scss; exit 0",
    "js-fix": "echo \"=========== JS-FIX ===========\"; standard --fix --verbose | snazzy; exit 0",
    "fixes": "npm run css-lint && npm run js-fix"
  },
  "keywords": [
    "react",
    "babel",
    "es6",
    "sass",
    "webpack"
  ],
  "author": "Pedro Gaspar",
  "license": "ISC",
  "repository": "https://github.com/pedrogasparcs/io.offstage.frontend-starter-kit.git",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-ie8": "^1.4.3",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.2.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.8.0",
    "querystring": "^0.2.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.2",
    "redux-logger": "^3.0.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "svgo": "^0.6.1",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "snazzy": "^6.0.0",
    "standard": "^10.0.0",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "qs": "^6.4.0"
  }
}
